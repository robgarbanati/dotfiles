Host gateway.ln
    Hostname gateway.lyft.net
    ProxyCommand none
    GatewayPorts yes

Host *-prod
    Hostname %huction-iad.lyft.net

# EC2 Instance ID
Host i-*
    Hostname %h.lyft.net

Host gateway-sfo.ln
	ProxyCommand none
	Hostname gateway-sfo.lyft.net
	GatewayPorts yes

Host *sfo*.lyft.net *sfo*.ln
	ProxyCommand ssh gateway-sfo.ln "proxy-ec2 %h"
	User zhsi
	ForwardAgent yes
 
Host *.lyft.me *.lm *.lyft.net *.ln *-prod i-*
    ProxyCommand ssh gateway.ln "proxy-ec2 %h"
    User zhsi
    ForwardAgent yes
 
Host *
    KeepAlive yes
    ServerAliveInterval 60
    GSSAPIAuthentication no
    ForwardAgent no
    ForwardX11 no
    UseRoaming no
